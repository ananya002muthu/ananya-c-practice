#include <iostream>
using namespace std;
int main()
{
    int n,count;
    cin>>n;
    int m[n][n];
    for (int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            cin>>m[i][j];
        }
    }
            for(int i=0;i<n-1;i++)
            {
                for(int j=1;j<n;j++)
                {
                    if(m[i][j]==0)
                    {
                         count+=1;//count=count+1
                    }
                    else
                    {
                        count+=0;
                    }
                 }
            }
             if(count>=n)
        {
            cout<<"Lower triangular matrix";
        }
        else
        {
            cout<<"Not a Lower triangular matrix";
        }
}
