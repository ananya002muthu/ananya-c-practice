#include<iostream>
using namespace std;
int main()
{
    char a;
    cin>>a;
    if((a>=65)&&(a<=90))
    {
    cout<<"Upper";
    }
   else if((a>=0)&&(a<=9))
    {
        cout<<"Number";
    }
    else if((a>=97)&&(a<=122))
    {
     cout<<"Lower";   
    }
    else
    {
        cout<<"Symbol"; 
    }
}
