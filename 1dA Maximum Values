#include<iostream>
using namespace std;
int main()
{
    int a,j;
    cin>>a;
    int m[a];
    for (int i=0;i<a;i++)
    {
        cin>>m[i];
    }
   /** for (int i=0;i<a;i++)
    {
        cout<<m[i]<<endl;
    }**/
    for (int i=0;i<a;i++)
    {
        if (m[0]<m[i])
        m[0]=m[i];
    }
    cout<<m[0];
   
}
