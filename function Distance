#include<iostream>
#include<cmath>
using namespace std;
float distance(int n1,int n2,int n3,int n4)
{
    int d1,d2;
    float d;
    int s1,s2,s;
    d1=n4-n2;
    d2=n3-n1;
    s1=d1*d1;
    s2=d2*d2;
    s=s1+s2;
    d=sqrt(s);
    return d;

}
int main()
{
    int n1,n2,n3,n4;
    cin>>n1>>n2>>n3>>n4;
    float x=distance(n1,n2,n3,n4);
    printf("Distance between the 2 points is %0.2f",x);
}
