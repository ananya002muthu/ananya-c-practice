#include <iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int m[n],o[n];
    
    for(int i=0;i<n;i++)
    {
        cin>>m[i];
    }
    for (int i=0;i<n;i++)
    {
        cin>>o[i];
    }
    int sum[n];
    for(int i=0;i<n;i++)
    {
        sum[i]=m[i]+o[i];
        cout<<sum[i]<<" ";
    }
    
}
